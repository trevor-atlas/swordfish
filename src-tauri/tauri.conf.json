{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "swordfish",
  "mainBinaryName": "swordfish",
  "version": "0.0.1",
  "identifier": "com.swordfish.dev",
  "plugins": { },
  "app": {
    "macOSPrivateApi": true,
    "windows": [],
    "withGlobalTauri": true,
    "trayIcon": {
      "iconPath": "icons/32x32.png"
    },
    "security": {
      "assetProtocol": {
        "scope": [
          "**"
        ],
        "enable": true
      },
      "csp": null
    }
  }
}
